{
  "name": "mqtt2ha",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "description": "Javascript library to create MQTT entities that are automatically discovered by Home Assistant.",
  "keywords": [
    "homeassistant",
    "home-assistant",
    "hass",
    "mqtt",
    "discovery",
    "mqtt-discovery"
  ],
  "publishConfig": {
    "provenance": true
  },
  "author": {
    "name": "Pascal Bourque",
    "email": "pascal@cosmos.moi"
  },
  "bugs": {
    "url": "https://github.com/bourquep/mqtt2ha/issues"
  },
  "homepage": "https://github.com/bourquep/mqtt2ha",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bourquep/mqtt2ha.git"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.4.0"
  },
  "browser": false,
  "scripts": {
    "example": "tsx --watch ./test-environment/example.ts",
